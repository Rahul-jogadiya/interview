{
  "name": "interview",
  "version": "1.0.0",
  "description": "project setUp for Sql database and sequelize",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "npx rimraf ./build",
    "start": "node build/server.js",
    "zip": "tar.exe -a -c -f foodMall.zip build package.json config .platform .npmrc .ebextensions",
    "deploy": "start deploy.bat",
    "ebs": "start ebs.bat",
    "dist": "sh ./scripts/build.sh",
    "server": "tsc-watch --outDir ./build --onSuccess \"npm run start -- --mode=http\" --onFailure \"echo ERROR! Compilation Failed\""
  },
  "keywords": [
    "SQL",
    "Sequelize",
    "Database"
  ],
